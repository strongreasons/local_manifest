<?xml version="1.0" encoding="UTF-8"?>

<manifest>

        <remote name="znieb"
        fetch="https://github.com/znieb" />

        <remote name="Kneba"
        fetch="https://github.com/Kneba" />

        <remote name="strongreasons"
        fetch="https://github.com/strongreasons" />

        <remote name="SagarMakhar"
        fetch="https://github.com/SagarMakhar" />

        <remote name="Tiktodz"
        fetch="https://github.com/Tiktodz" />

        <!-- Sync trees -->
        <project path="device/asus/X00TD" name="device_asus_X00TD" remote="strongreasons" revision="pie" />
        <project path="vendor/asus" name="proprietary_vendor_asus" remote="SagarMakhar" revision="lineage-16.0" />
        <project path="kernel/asus/sdm660" name="android_kernel_asus_sdm660" remote="strongreasons" revision="9" />

        <remove-project name="LineageOS/android_build" />
        <project path="build/make" name="android_build" remote="Kneba" revision="lineage-16.0" groups="pdk" >
        <copyfile src="core/root.mk" dest="Makefile" />
        <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
        <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
        <linkfile src="core" dest="build/core" />
        <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
        <linkfile src="target" dest="build/target" />
        <linkfile src="tools" dest="build/tools" />
        </project>

        <!-- Frameworks base -->
        <remove-project name="LineageOS/android_frameworks_base" />
        <project path="frameworks/base" name="android_frameworks_base" remote="Kneba" revision="lineage-16.0" />

        <!-- Overlays -->
        <remove-project name="LineageOS/android_packages_overlays_Lineage" />
        <project path="packages/overlays/Lineage" name="android_packages_overlays_Lineage" remote="Tiktodz" revision="9" />

        <!-- Settings -->
        <remove-project name="LineageOS/android_packages_apps_Settings" />
        <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="strongreasons" revision="lineage-16.0" />

</manifest>
